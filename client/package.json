    "autoprefixer": "^9.7.6",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.8.2",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "5.3.2",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react-icons": "^4.2.0",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "5.51.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/**.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/coverage/*",
      "/dist/*",
      "/node_modules/*",
      "/schemas/*",
      "/templates/*",
      "/test/*",
      "/webpack.config.js"
    ],
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^.+\\.(png|svg|gif|jpg|jpeg)$": "<rootDir>/test/jestConfig/require_stub.js",
      "^.+\\.(css|scss)$": "<rootDir>/test/jestConfig/require_stub.js"
    },
    "automock": false,
    "setupFiles": [
      "<rootDir>/test/jestConfig/setupJest.js"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
